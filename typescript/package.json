{
  "name": "actionableagileimporttool",
  "version": "1.2.1",
  "description": "Tool to extract data from JIRA into a format understood by the ActionableAgile metrics tool",
  "main": "dist/main.js",
  "typings": "dist/main",
  "scripts": {
    "tsc:w": "tsc -w",
    "tsc:version": "tsc --version",
    "typings-install": "typings install",
    "build": "tsc",
    "install-typings": "npm run typings-install",
    "jira-extract": "node dist/cli",
    "start": "npm run tsc:w",
    "test": "mocha src/test --recursive --compilers ts:ts-node/register --timeout 10000",
    "lint": "tslint ./src/**/*.ts",
    "outDir": "dist/",
    "build-bundle": "npm run build && browserify dist/cli.js --node --standalone rawjs > jira-to-analytics.js",
    "build-executable": "nexe -i dist/cli.js -o jira.exe --flags",
    "build-release": "npm run build-bundle"
  },
  "keywords": [
    "typescript",
    "javascript",
    "agile",
    "actionableagile",
    "scrum",
    "kanban",
    "jira",
    "extract",
    "nodejs",
    "node",
    "yawml",
    "tool",
    "metrics"
  ],
  "author": "johnrjj@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^6.0.45",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "ts-node": "^0.7.3",
    "typescript": "^2.0.3",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "@types/inquirer": "0.0.29",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/js-yaml": "^3.5.28",
    "@types/progress": "^1.1.28",
    "@types/request": "0.0.31",
    "@types/yargs": "0.0.33",
    "core-js": "^2.4.0",
    "inquirer": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.6.1",
    "leankit-client": "^1.1.2",
    "moment": "^2.14.1",
    "progress": "^1.1.8",
    "request": "^2.75.0",
    "try-thread-sleep": "^1.0.0",
    "typescript": "^2.0.3",
    "yargs": "^4.7.1"
  }
}
